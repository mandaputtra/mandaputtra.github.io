<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ renderData.title or title or metadata.title }}</title>
    <meta property="og:title" content="{{ renderData.title or title or metadata.title }}">

    <meta property="description" content="{{ renderData.description or description or metadata.description }}">
    <meta property="og:description" content="{{ renderData.description or description or metadata.description }}">
    <link rel="stylesheet" href="{{ '/css/index.css' | url }}">
    <link rel="stylesheet" href="{{ '/css/main.css' | url }}">
    <link rel="stylesheet" href="{{ '/css/prism-base16-monokai.dark.css' | url }}">
    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}">
    <link rel="shortcut icon" href="{{ metadata.favicon }}" type="image/x-icon">
    <link rel="icon" href="{{ metadata.favicon }}" type="image/x-icon">
    
    <meta property="twitter:image" content="{{metadata.social_image}}">
    <meta property="twitter:card" content="summary">
    <meta property="og:image:width" content="{{metadata.social_image_width}}">
    <meta property="og:image:height" content="{{metadata.social_image_height}}">
    <meta property="og:image" content="{{metadata.social_image}}">
    <meta property="twitter:image:alt" content="Learning is fun!">
  </head>
  <body>
    <header class='site-header'>
      <div class="wrapper">
        <h1><a class='site-title' href="{{ '/' | url }}">{{ metadata.heading }}</a></h1>
        <ul class="nav site-nav">
          {%- for nav in collections.nav | reverse -%}
          <li class="nav-item{% if nav.url == page.url %} nav-item-active{% endif %}"><a href="{{ nav.url | url }}">{{ nav.data.navtitle }}</a></li>
          {%- endfor -%}
        </ul>
      </div>
    </header>

    <main class="page-content">
    <div class="content-wrapper wrapper">
      {{ content | safe }}
      </div>
    </main>

    <footer class="site-footer">

  <div class="wrapper">

    <h2 class="footer-heading">{{ metadata.title }}</h2>

    <div class="footer-col-wrapper">

      <div class="footer-col footer-col-2">
        <ul class="social-media-list">
            {%- if metadata.author.glitch_username -%}
            <li>
              <a href="https://glitch.com/@{{ metadata.author.glitch_username }}"><img height="20" width="20" src="https://cdn.glitch.com/238f8585-6bd5-40c4-a0ff-2b87d4acea6c%2Fglitch-fish-night.png" alt='Glitch' /><span class="username">Glitch Profile</span></a>
            </li>
            {%- endif -%}

          {%- if metadata.author.github_username -%}
          <li>
            <a href="https://github.com/{{ metadata.author.github_username }}"><span class="icon icon--github"><svg viewBox="0 0 16 16"><path fill="#828282" d="M7.999,0.431c-4.285,0-7.76,3.474-7.76,7.761 c0,3.428,2.223,6.337,5.307,7.363c0.388,0.071,0.53-0.168,0.53-0.374c0-0.184-0.007-0.672-0.01-1.32 c-2.159,0.469-2.614-1.04-2.614-1.04c-0.353-0.896-0.862-1.135-0.862-1.135c-0.705-0.481,0.053-0.472,0.053-0.472 c0.779,0.055,1.189,0.8,1.189,0.8c0.692,1.186,1.816,0.843,2.258,0.645c0.071-0.502,0.271-0.843,0.493-1.037 C4.86,11.425,3.049,10.76,3.049,7.786c0-0.847,0.302-1.54,0.799-2.082C3.768,5.507,3.501,4.718,3.924,3.65 c0,0,0.652-0.209,2.134,0.796C6.677,4.273,7.34,4.187,8,4.184c0.659,0.003,1.323,0.089,1.943,0.261 c1.482-1.004,2.132-0.796,2.132-0.796c0.423,1.068,0.157,1.857,0.077,2.054c0.497,0.542,0.798,1.235,0.798,2.082 c0,2.981-1.814,3.637-3.543,3.829c0.279,0.24,0.527,0.713,0.527,1.437c0,1.037-0.01,1.874-0.01,2.129 c0,0.208,0.14,0.449,0.534,0.373c3.081-1.028,5.302-3.935,5.302-7.362C15.76,3.906,12.285,0.431,7.999,0.431z"/></svg>
            </span><span class="username">GitHub</span></a>
          </li>
          {%- endif -%}
          {%- if metadata.author.site -%}
          <li>
            <a href='{{metadata.author.site}}'>{{metadata.author.name}}'s Main Site</a>
          </li>
          {%- endif -%}
          {%- for twitterAccount in metadata.author.twitters -%}
          <li>
            
          </li>
          {%- endfor -%}
        </ul>
      </div>

      <div class="footer-col footer-col-3 description">
        <p>{{ site.description }}</p>
      </div>
    </div>

  </div>

</footer>
    <img src="https://cassey-lytics.glitch.me/counter.png?fallback=cassey-til" alt="" style="vertical-align: bottom; visibility:hidden" aria-hidden="true">

    <!-- Current page: {{ page.url | url }} -->
  </body>
</html>
